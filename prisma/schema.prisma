
generator client {
  provider = "prisma-client-js"
  output   = "./generated"
  
}

datasource db {
  provider = "postgresql"
}


model BlogPost {
  id          String @id @default(uuid())
  title       String
  content     String
  imageUrl    String
  authorId    String
  authorName  String
  authorImage String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}